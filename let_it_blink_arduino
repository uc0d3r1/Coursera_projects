void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
}
void loop() {

  for(int i = 0; i < 5; i++) {
    digitalWrite(LED_BUILTIN, HIGH);   
    delay(250);              
    digitalWrite(LED_BUILTIN, LOW);    
    delay(250);              
  }
  for(int i = 0; i < 5; i++) {
    digitalWrite(LED_BUILTIN, HIGH);  
    delay(1000);              
    digitalWrite(LED_BUILTIN, LOW);    
    delay(1000);              
  }
}
